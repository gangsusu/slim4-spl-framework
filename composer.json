{
    "name": "spl/spl-framework",
    "description": " ",
    "keywords": ["framework", "spl-framework"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "splanzg13163",
            "email": "splanzg13163@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "slim/slim": "^4.0",
        "slim/http": "^0.8",
        "slim/psr7": "^0.5",
        "illuminate/database": "^6.0",
        "illuminate/config": "^6.0",
        "illuminate/filesystem": "^6.0",
        "illuminate/validation": "^6.0",
        "illuminate/cache": "^6.0",
        "illuminate/container": "^6.0",
        "symfony/console": "^4.3",
        "monolog/monolog": "^2.0",
        "symfony/var-dumper": "^4.3",
        "vlucas/phpdotenv": "^3.6",
        "hprose/hprose": "^2.0",
        "zeuxisoo/slim-whoops": "0.7.*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "friendsofphp/php-cs-fixer": "^2.15"
    },
    "autoload": {
        "classmap": [
            "app/Models"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S 0.0.0.0:8080 -t public public/index.php",
        "phpcs": "vendor/bin/php-cs-fixer fix"
    }
}
